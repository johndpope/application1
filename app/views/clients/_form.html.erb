		<div class="col-md-12">
			<div class="form-group">
				<%= f.check_box :is_active, class: 'iCheck-helper' %>
				<%= f.label :is_active, class: 'control-label' %>
				<%= tooltip_by_field({tooltip_text: t("tooltips.client.is_active", :default => "")}) %>
			</div>
		</div>
	</div>

