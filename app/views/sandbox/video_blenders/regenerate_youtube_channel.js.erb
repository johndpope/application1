<% unless @channel_arts.first.blank? %>
$('.channel_art_selected').attr("src",'<%=@channel_arts.first.file.url%>');
<% end %>

<% unless @video_thumbnails.first.blank? %>
$('.selected_thumbnail').attr("src",'<%=@video_thumbnails.first.file.url%>');
$('body').find('.selected_thumbnail').attr('data-id', '<%=@video_thumbnails.first.id%>');
<% end %>

<% unless @channel_icon.blank? %>
  $('.channel_icon_img').attr('src','<%=@channel_icon.file.url%>');
<% end %>

<%
  channel_name_component = render partial: "sandbox/video_blenders/youtube_setup/channel_setup/channel_name_component", locals: {channel_schema: @channel_schema}
%>
$('.channel_name_components').html('<%= escape_javascript(channel_name_component) %>');
$('h3.channel_name').text('<%=@channel_name%>');

<% unless @video_thumbnails.blank? %>
  src = $('#video1_html5_api').attr('src');
  $('body').find('#video2_html5_api').attr('src', src).attr('poster','<%=@video_thumbnails.first.file.url%>');
  $('body').find('#video3_html5_api').attr('src', src).attr('poster','<%=@video_thumbnails.first.file.url%>');
<% end %>
